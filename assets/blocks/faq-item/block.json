{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "usercentrics-custom-blocks/faq-item",
	"version": "0.1.0",
	"title": "FAQ Item",
	"category": "widgets",
	"icon": "smiley",
	"description": "A block to display a single FAQ item.",
	"example": {},
	"supports": {
		"html": false
	},
	"parent": [
		"usercentrics-custom-blocks/faq-list"
	],
	"allowedBlocks": [
		"core/paragraph",
		"core/list"
	],
	"attributes": {
		"title": {
			"type": "string",
			"default": "FAQ Item Title"
		},
		"openByDefault": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "usercentrics-custom-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}